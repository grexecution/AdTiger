{
  "name": "adfire",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && node scripts/simple-setup.cjs && next build",
    "setup-db": "prisma db push && node scripts/vercel-setup.cjs",
    "build:full": "pnpm --filter=shadcn build && contentlayer2 build && pnpm build:registry && next build",
    "build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --loglevel silent --write \"registry/**/*.{ts,tsx,mdx}\" --cache",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --loglevel silent --write \"registry/**/*.{ts,tsx,mdx}\" --cache",
    "registry:capture": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/capture-registry.mts",
    "build:docs": "contentlayer2 build",
    "seed:tasks": "tsx --tsconfig ./tsconfig.scripts.json ./app/(app)/examples/tasks/data/seed.ts",
    "db:generate": "npx dotenv -e .env.local -- prisma generate",
    "db:migrate": "npx dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "worker": "npx dotenv -e .env.local -- tsx scripts/start-worker.ts",
    "worker:dev": "npx dotenv -e .env.local -- tsx --watch scripts/start-worker.ts",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:seed": "npx dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:studio": "npx dotenv -e .env.local -- prisma studio",
    "db:setup": "npm run db:push && npm run db:seed",
    "workers": "npx dotenv -e .env.local -- tsx lib/jobs/worker-process.ts",
    "workers:dev": "npx dotenv -e .env.local -- tsx --watch lib/jobs/worker-process.ts",
    "start": "next start -p 3333",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start -p 3333",
    "typecheck": "contentlayer2 build && tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache"
  },
